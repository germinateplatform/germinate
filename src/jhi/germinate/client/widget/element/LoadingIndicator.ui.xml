<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
			 xmlns:b='urn:import:org.gwtbootstrap3.client.ui'
			 xmlns:h='urn:import:org.gwtbootstrap3.client.ui.html'
			 xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:FlowPanel>
		<b:Modal closable="false" fade="true" removeOnHide="true" dataBackdrop="STATIC" ui:field="modal">
			<b:ModalBody ui:field="body">
				<b:Progress active="true" type="STRIPED">
					<b:ProgressBar percent="100" type="INFO" ui:field="progress"/>
				</b:Progress>
				<h:Paragraph emphasis="MUTED" ui:field="title"/>
			</b:ModalBody>
		</b:Modal>
	</g:FlowPanel>
</ui:UiBinder>